    "\n",
    "# AVALIAÇÃO DATA SCIENCE\n",
    "# NOTEBOOK COLAB https://colab.research.google.com/drive/1tfIDdavjN3UtQ5uBo00_zrS4IXLoGlnX?usp=sharing\n"
